; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Puffer"
run/main_scene="res://Main.tscn"
boot_splash/image="res://grumble.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Global.cs"
Menu="*res://Menu.tscn"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=360
window/size/height=740
window/size/always_on_top=true
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "MonoDebuggerQM-1.0.1" )

[gui]

theme/use_hidpi=true

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[logging]

file_logging/enable_file_logging=true
file_logging/max_log_files=2

[rendering]

quality/filters/anisotropic_filter_level=16
quality/shadows/filter_mode=2
quality/shadows/filter_mode.mobile=2
quality/subsurface_scattering/quality=2
quality/voxel_cone_tracing/high_quality=true
quality/filters/msaa=4
quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
